
<launch>
 
 <group ns="robot2">
    <param name="robot_description"
   command="$(find xacro)/xacro.py $(find turtlebot_description)/robots/kobuki_hexagons_asus_xtion_pro.urdf.xacro" />
    <param name="tf_prefix" value="robot2_tf" />
 
    <node name="spawn_turtlebot2_model" pkg="gazebo_ros" type="spawn_model"
        args="$(optenv ROBOT_INITIAL_POSE) -unpause -urdf -param robot_description -model turtlebot"/>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="30.0" />
    </node>
   
    
  </group>
</launch>
